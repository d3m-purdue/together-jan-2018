{
  "name": "d3m-ta3-modeling",
  "version": "0.1.0",
  "description": "A prototype TA3 application enabling a hypothetical modeling task",
  "main": "index.js",
  "scripts": {
    "data:small": "mkdir -p data/d3m; cd data/d3m; curl -J -O https://data.kitware.com/api/v1/file/59b1717d8d777f7d33e9d061/download && tar xzvf small.tar.gz && mv small/* . && rmdir small && rm small.tar.gz",
    "build": "webpack",
    "pythonprep": "virtualenv venv && ./venv/bin/pip install -r requirements.txt && ./venv/bin/pip install --no-build-isolation pandas==0.22.0",
    "rprep": "mkdir -p rlib && Rscript script/prep.R",
    "protobuf": "./venv/bin/python -m grpc_tools.protoc -I proto --python_out=src/tangelo --grpc_python_out=src/tangelo core.proto",
    "watch": "webpack -w",
    "lint": "semistandard | snazzy",
    "serve": "R_LIBS_SITE=$PWD/rlib:$R_LIBS_SITE  ./venv/bin/tangelo  -np --host 0.0.0.0 --root build --port ${PORT-8080}",
    "serve_d3m": "sudo nohup ./venv/bin/tangelo --host 0.0.0.0 --root build --port ${PORT-80} &",
    "kill_d3m": "sudo pkill -f tangelo",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d3m-purdue/d3m-ta3-modeling.git"
  },
  "author": "Roni Choudhury",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/d3m-purdue/d3m-ta3-modeling/issues"
  },
  "homepage": "https://github.com/d3m-purdue/d3m-ta3-modeling#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "bootstrap": "^3.3.7",
    "bootswatch": "^3.3.7",
    "@candela/core": "~0.23.3",
    "@candela/size": "~0.23.3",
    "@candela/vega": "~0.23.3",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "d3-selection": "^1.1.0",
    "datalib": "^1.7.3",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "javascript-detect-element-resize": "^0.5.3",
    "jquery": "^3.2.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "raw-loader": "^0.5.1",
    "redux": "^3.7.0",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "remarkable": "^1.7.1",
    "semistandard": "^11.0.0",
    "snazzy": "^7.0.0",
    "strip-json-comments-loader": "0.0.2",
    "style-loader": "^0.18.2",
    "telegraph-events": "^1.0.3",
    "url-loader": "^0.5.9",
    "vega": "3.0.9",
    "vega-embed": "^3.0.0",
    "vega-lite": "^2.1.2",
    "vega-scenegraph": "2.1",
    "vega-wordcloud": "2.0.2",
    "webpack": "^3.0.0",
    "yaml-loader": "^0.4.0"
  },
  "dependencies": {
    "npm": "^5.6.0"
  }
}
